{
  "name": "@easeml/jupyterlab_easeml",
  "version": "0.1.13",
  "description": "ease.ml client for jupyterlab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/DS3Lab/easeml",
  "bugs": {
    "url": "https://github.com/DS3Lab/easeml/issues"
  },
  "license": "MIT",
  "author": "DS3 lab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/DS3Lab/easeml.git"
  },
  "scripts": {
    "build": "webpack",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo && rimraf ds3-jupyterlab_easeml*.tgz",
    "prepare": "npm run clean && npm run build",
    "watch": "webpack --watch",
    "lint": "tslint --project ./ && vue-cli-service lint"
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@jupyterlab/application": "^1.2.1",
    "@jupyterlab/apputils": "^1.2.1",
    "@jupyterlab/launcher": "^1.2.1",
    "@phosphor/messaging": "^1.2.3",
    "@phosphor/widgets": "^1.8.1",
    "@types/node": "^12.12.27",
    "@vue/cli": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "autoprefixer": "^9.7.4",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.4.2",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "file-loader": "^1.1.11",
    "node-sass": "^4.13.1",
    "node-static": "^0.7.11",
    "postcss-loader": "^3.0.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "tslint": "^6.0.0",
    "ttf-loader": "^1.0.2",
    "typescript": "^3.7.5",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "files": [
    "schema/*.json"
  ]
}
