{
  "name": "easemlclient",
  "version": "0.1.0",
  "description": "The JavaScript client to the ease.ml system.",
  "main": "lib/easemlclient.js",
  "module": "src/index.js",
  "files": [
    "lib/**/*",
    "src/**/*",
    "dist/**/*"
  ],
  "scripts": {
    "rimraf": "./node_modules/rimraf/bin.js",
    "clean": "rimraf dist && rimraf lib && rimraf docs",
    "build": "npm run build:node && npm run build:browser",
    "build:node": "babel src --out-dir ./lib --source-maps",
    "build:node:watch": "babel src --out-dir ./lib --source-maps --watch",
    "build:browser": "webpack",
    "test": "npm run test:node",
    "test:node": "echo 'Add Unit Tests'",
    "dev": "npm run build:node:watch",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint --fix src/**/*.js",
    "doc": "mkdir -p docs && doxdox 'src/**/*.js' --layout markdown --output docs/client.md"
  },
  "keywords": [
    "easeml"
  ],
  "author": "Bojan Karlas",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.6.3",
    "babel-preset-env": "^1.7.0",
    "doxdox": "^2.0.3",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "handlebars": "^4.4.5",
    "lodash": "^4.17.12",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/register": "^7.9.0",
    "axios": "^0.19.0",
    "decamelize-keys": "^1.1.0",
    "mocha": "^7.1.2",
    "moment": "^2.24.0",
    "moment-duration-format": "^2.3.2",
    "tus-js-client": "^1.8.0",
    "url-join": "^4.0.1"
  }
}
